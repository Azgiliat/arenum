list = 'Light', 'Regular', 'Medium', 'Semibold', 'Bold', 'Black'
for font, i in list
  weight = 0
  if (font == 'Black')
    weight = 900
  else
    weight = (i + 3) * 100
  @font-face
    font-family: 'GraphikLCG'
    font-style: normal
    font-weight: weight
    src: local('GraphikLCG'), url('~st/fonts/GraphikLCG-' + list[i] + '.woff2') format('woff2'), url('~st/fonts/GraphikLCG-' + list[i] + '.woff') format('woff'), url('~st/fonts/GraphikLCG-' + list[i] + '.ttf') format('truetype');
